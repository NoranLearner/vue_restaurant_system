tate);\n        return { state, v$ };\n    },\n    data() {\n        return {\n            \n        }\n    },\n    methods: {\n        async addLocation() {\n            // Activate Validation\n            this.v$.$validate();\n            if (!this.v$.$error) {\n                console.log(\"Form Validated Successfully\");\n            } else {\n                console.warn(\"Form Validation Failed\");\n            }\n        },\n    },\n};\n</script>\n\n<style></style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}