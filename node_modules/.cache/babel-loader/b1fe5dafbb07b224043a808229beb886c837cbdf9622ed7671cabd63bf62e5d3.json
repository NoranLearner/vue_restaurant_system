{"ast":null,"code":"import axios from \"axios\";\nimport Navbar from \"@/components/Header/Navbar.vue\";\nimport AddNewLocation from \"@/components/Locations/AddNewLocation.vue\";\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      userName: \"\",\n      listOfLocations: []\n    };\n  },\n  components: {\n    Navbar,\n    AddNewLocation\n  },\n  async mounted() {\n    let user = localStorage.getItem('user-info');\n    if (!user) {\n      // Using name in router file\n      this.redirectTo({\n        val: 'sign-up'\n      });\n    } else {\n      this.userName = JSON.parse(user).name;\n    }\n    let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n    if (result.status == 200 && result.data.length > 0) {\n      this.listOfLocations = result.data;\n    }\n  },\n  methods: {\n    ...mapActions([\"redirectTo\"])\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,gCAAgC;AACnD,OAAOC,cAAa,MAAO,2CAA2C;AACtE,SAASC,UAAS,QAAS,MAAM;AACjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IAAEP,MAAM;IAAEC;EAAe,CAAC;EACtC,MAAMO,OAAO,GAAG;IACd,IAAIC,IAAG,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IAC5C,IAAI,CAACF,IAAI,EAAE;MACT;MACA,IAAI,CAACG,UAAU,CAAC;QAAEC,GAAG,EAAE;MAAU,CAAC,CAAC;IACrC,OAAO;MACL,IAAI,CAACR,QAAO,GAAIS,IAAI,CAACC,KAAK,CAACN,IAAI,CAAC,CAACN,IAAI;IACvC;IACA,IAAIa,MAAK,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAE,0CAAyC,IAAI,CAACC,MAAO,EAAC,CAAC;IACrF,IAAIF,MAAM,CAACG,MAAK,IAAK,GAAE,IAAKH,MAAM,CAACZ,IAAI,CAACgB,MAAK,GAAI,CAAC,EAAE;MAClD,IAAI,CAACd,eAAc,GAAIU,MAAM,CAACZ,IAAI;IACpC;EACF,CAAC;EACDiB,OAAO,EAAE;IACP,GAAGnB,UAAU,CAAC,CAAC,YAAY,CAAC;EAC9B;AACF,CAAC","names":["axios","Navbar","AddNewLocation","mapActions","name","data","userName","listOfLocations","components","mounted","user","localStorage","getItem","redirectTo","val","JSON","parse","result","get","userId","status","length","methods"],"sourceRoot":"","sources":["/opt/lampp/htdocs/rest-sys-2021/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <Navbar />\n    <p class=\"text-end\">\n      Welcome {{ userName }}\n      <router-link :to=\"{ name: 'profile' }\">\n        <button class=\"btn btn-info text-white\" type=\"button\">Profile</button>\n      </router-link>\n    </p>\n  </div>\n  <AddNewLocation/>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Navbar from \"@/components/Header/Navbar.vue\";\nimport AddNewLocation from \"@/components/Locations/AddNewLocation.vue\";\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      userName: \"\",\n      listOfLocations: [],\n    };\n  },\n  components: { Navbar, AddNewLocation },\n  async mounted() {\n    let user = localStorage.getItem('user-info');\n    if (!user) {\n      // Using name in router file\n      this.redirectTo({ val: 'sign-up' });\n    } else {\n      this.userName = JSON.parse(user).name;\n    }\n    let result = await axios.get(`http://localhost:3000/locations?userId=${this.userId}`);\n    if (result.status == 200 && result.data.length > 0) { \n      this.listOfLocations = result.data;\n    }\n  },\n  methods: {\n    ...mapActions([\"redirectTo\"]),\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}